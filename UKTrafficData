      "source": [
        "#df['Date'] = pd.to_datetime(df['Date'], format='%d/%m/%y')\n",
        "#df['Day'] = df['Date'].dt.day\n",
        "df['Hour of Day'] = pd.to_datetime(df['Time']).dt.hour\n",
        "df['Month'] = pd.to_datetime(df['Date']).dt.month\n",
        "\n",
        "\n",
        "AccidentPerWeek = df.groupby(['Day_of_Week']).sum()['Number_of_Casualties'].to_frame().reset_index()\n",
        "AccidentsPerWeek = df.groupby(['Date', 'Day_of_Week', 'Month']).sum()['Number_of_Casualties'].to_frame().reset_index()\n",
        "\n",
        "AccidentsByHour = df.groupby(['Day_of_Week', 'Hour of Day']).sum()['Number_of_Casualties'].to_frame().reset_index()\n",
        "AccidentsByHour_pivot = AccidentsByHour.pivot_table(values=['Number_of_Casualties'], index=['Hour of Day'], columns=['Day_of_Week'])\n",
        "Days = ['Sunday','Monday','Tuesday','Wednesday','Thursday','Friday','Saturday']\n",
        "\n",
        "# Plot bars for Number of Casualties by Day of Week\n",
        "plt.figure(figsize=(10,6))\n",
        "sns.set_style('darkgrid')\n",
        "b = sns.barplot(x='Day_of_Week', y='Number_of_Casualties', data=AccidentPerWeek, linewidth=1, edgecolor='black')\n",
        "\n",
        "x_axis = np.arange(0,7,1)\n",
        "y_max = max(AccidentPerWeek['Number_of_Casualties'])\n",
        "y_max -= y_max % -1000\n",
        "step = 1000\n",
        "y_axis = np.arange(2000, y_max+step, step)\n",
        "y_axis_fmt = [\"{:,.0f}\".format(y) for y in y_axis]\n",
        "plt.ylim(min(y_axis), max(y_axis))\n",
        "\n",
        "plt.xticks(x_axis, ('Sunday','Monday','Tuesday','Wednesday','Thursday','Friday','Saturday'), fontsize=13)\n",
        "plt.yticks(y_axis, y_axis_fmt, fontsize=13)\n",
        "plt.xlabel('')\n",
        "plt.ylabel('Number of Accidents', fontsize=13)\n",
        "\n",
        "# Add Data Labels for bar values\n",
