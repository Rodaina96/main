        "import seaborn as sns\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "#df['Date'] = pd.to_datetime(df['Date'], format='%d/%m/%y')\n",
        "#df['Month'] = df['Date'].dt.month\n",
        "month_list = ['January', 'February', 'March', 'April', 'May', 'June', \n",
        "              'July', 'August', 'September', 'October', 'November', \n",
        "              'December']\n",
        "monthLength_list = [31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31]\n",
        "trafficDataByMonth_df['Month Length'] = monthLength_list\n",
        "AccidentsPerMonth = trafficDataByMonth_df['Accident Count']/trafficDataByMonth_df['Month Length']\n",
        "\n",
        "trafficDataByMonth_df = df[['Date', 'Accident_Index']].copy()\n",
        "trafficDataByMonth_df.index = trafficDataByMonth_df['Date']\n",
        "trafficDataByMonth_df.index = pd.to_datetime(trafficDataByMonth_df.index)\n",
        "trafficDataByMonth_df = pd.DataFrame(trafficDataByMonth_df.resample('M').count()['Accident_Index'])\n",
        "trafficDataByMonth_df = trafficDataByMonth_df.reset_index()[['Date', 'Accident_Index']]\n",
        "trafficDataByMonth_df.rename(columns={'Accident_Index':'Accident Count'}, inplace=True )\n",
        "\n",
        "\n",
        "sns.set()\n",
        "plt.rcParams['figure.figsize'] = [15,5]\n",
        "plt.bar(trafficDataByMonth_df['Date'],AccidentsPerMonth, width=15, align='center', linewidth=1, edgecolor='black', tick_label=month_list, alpha=0.75)\n",
        "plt.title(\"Normalized Accident Count by Month\", size=16)\n",
        "plt.xlabel(\"Month\", size=13)\n",
        "plt.ylabel(\"Normalized Accident Count\", size=13)\n",
        "\n",
        "trafficDataByMonth_df['Date'] = month_list\n",
        "\n",
        "plt.show()\n",
